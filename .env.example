# Environment Variables Example
# Copy this file to .env and update values as needed

# Application
REACT_APP_NAME=Paradise Nursery
REACT_APP_DESCRIPTION=Your Ultimate Plant Shopping Destination
REACT_APP_VERSION=1.0.0

# API Configuration
REACT_APP_API_URL=http://localhost:5000/api
REACT_APP_API_TIMEOUT=5000

# Features
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_ENABLE_WISHLIST=false

# Company Info
REACT_APP_COMPANY_NAME=Paradise Nursery
REACT_APP_COMPANY_EMAIL=info@paradisenursery.com
REACT_APP_COMPANY_PHONE=1-800-PLANTS-1
REACT_APP_COMPANY_ADDRESS=123 Green Street, Garden City, GC 12345

# Social Media
REACT_APP_FACEBOOK_URL=https://facebook.com/paradisenursery
REACT_APP_INSTAGRAM_URL=https://instagram.com/paradisenursery
REACT_APP_TWITTER_URL=https://twitter.com/paradisenursery

# Payment Gateway (Future)
REACT_APP_STRIPE_PUBLIC_KEY=your_stripe_key_here
REACT_APP_PAYPAL_CLIENT_ID=your_paypal_id_here
